        <Tab id="target-system">
          <GuiLabels guid="aabbccdd-eeff-1122-3344-556677889900">
            <Label language="EN">Target Configuration</Label>
            <Label language="DE">Target Konfiguration</Label>
          </GuiLabels>
          <AttributeGroup id="targetSystemSettings">
            <Name>Target System Settings</Name>
            <GuiLabels guid="99887766-5544-3322-1100-aabbccddeeff">
              <Label language="EN">Target System Settings</Label>
              <Label language="DE">Target System Einstellungen</Label>
            </GuiLabels>
            <AttributeReference>
              <ReferenceName>targetSystem</ReferenceName>
              <description>Please choose your target system...</description>
            </AttributeReference>

            <!--Salesforce authentication type-->
            <AttributeReference>
              <ReferenceName>salesforceAuth</ReferenceName>
              <EditCondition>
                <AttributeName>targetSystem</AttributeName>
                <AttributeValue>Salesforce</AttributeValue>
              </EditCondition>
            </AttributeReference>

            <!--WireMock authentication type-->
            <AttributeReference>
              <ReferenceName>wiremockAuth</ReferenceName>
              <EditCondition>
                <AttributeName>targetSystem</AttributeName>
                <AttributeValue>WireMock</AttributeValue>
              </EditCondition>
            </AttributeReference>
            <AttributeReference>
              <ReferenceName>httpMethod</ReferenceName>
              <description>Please choose your http method (GET, POST, DELETE, UPDATE, PUT, HEAD, OPTION)</description>
            </AttributeReference>
            <AttributeReference>
              <ReferenceName>url</ReferenceName>
              <description>Please input your instance url (https://your-domain.com)</description>
            </AttributeReference>
            <AttributeReference>
              <ReferenceName>tokenEndpoint</ReferenceName>
              <description>Please input your token endpoint (https://your-domain.com/oauth2/token)</description>
              <EditCondition>
                <AttributeName>targetSystem</AttributeName>
                <AttributeValue>Salesforce</AttributeValue>
              </EditCondition>
            </AttributeReference>
            <!--Field for salesforce auth type password-->
            <AttributeReference>
              <ReferenceName>username</ReferenceName>
              <OrCondition>
                <AndCondition>
                  <EditCondition>
                    <AttributeName>targetSystem</AttributeName>
                    <AttributeValue>Salesforce</AttributeValue>
                  </EditCondition>
                  <EditCondition>
                    <AttributeName>salesforceAuth</AttributeName>
                    <AttributeValue>password</AttributeValue>
                  </EditCondition>
                </AndCondition>
                <AndCondition>
                  <EditCondition>
                    <AttributeName>targetSystem</AttributeName>
                    <AttributeValue>WireMock</AttributeValue>
                  </EditCondition>
                  <EditCondition>
                    <AttributeName>wiremockAuth</AttributeName>
                    <AttributeValue>basic</AttributeValue>
                  </EditCondition>
                </AndCondition>
              </OrCondition>
            </AttributeReference>

            <AttributeReference>
              <ReferenceName>password</ReferenceName>
                <OrCondition>
                  <AndCondition>
                    <EditCondition>
                      <AttributeName>targetSystem</AttributeName>
                      <AttributeValue>Salesforce</AttributeValue>
                    </EditCondition>
                    <EditCondition>
                      <AttributeName>salesforceAuth</AttributeName>
                      <AttributeValue>password</AttributeValue>
                    </EditCondition>
                  </AndCondition>
                  <AndCondition>
                    <EditCondition>
                      <AttributeName>targetSystem</AttributeName>
                      <AttributeValue>WireMock</AttributeValue>
                    </EditCondition>
                    <EditCondition>
                      <AttributeName>wiremockAuth</AttributeName>
                      <AttributeValue>basic</AttributeValue>
                    </EditCondition>
                  </AndCondition>
                </OrCondition>
            </AttributeReference>

            <AttributeReference>
              <ReferenceName>clientId</ReferenceName>
              <AndCondition>
                <EditCondition>
                  <AttributeName>targetSystem</AttributeName>
                  <AttributeValue>Salesforce</AttributeValue>
                </EditCondition>
                <OrCondition>
                  <EditCondition>
                    <AttributeName>salesforceAuth</AttributeName>
                    <AttributeValue>password</AttributeValue>
                  </EditCondition>
                  <EditCondition>
                    <AttributeName>salesforceAuth</AttributeName>
                    <AttributeValue>client_credentials</AttributeValue>
                  </EditCondition>
                </OrCondition>
              </AndCondition>
            </AttributeReference>
            <AttributeReference>
              <ReferenceName>clientSecret</ReferenceName>
              <AndCondition>
                <EditCondition>
                  <AttributeName>targetSystem</AttributeName>
                  <AttributeValue>Salesforce</AttributeValue>
                </EditCondition>
                <OrCondition>
                  <EditCondition>
                    <AttributeName>salesforceAuth</AttributeName>
                    <AttributeValue>password</AttributeValue>
                  </EditCondition>
                  <EditCondition>
                    <AttributeName>salesforceAuth</AttributeName>
                    <AttributeValue>client_credentials</AttributeValue>
                  </EditCondition>
                </OrCondition>
              </AndCondition>
            </AttributeReference>
          </AttributeGroup>
